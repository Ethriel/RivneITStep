@model IEnumerable<GameStoreUI.Areas.Admin.Models.Developers.DeveloperViewModel>
@using Microsoft.AspNet.Identity
@{
    var isAdmin = User.IsInRole("Admin");
}
@{
    ViewBag.Title = "Developers";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

@if (isAdmin)
{
    Html.RenderPartial("Partial/Sidebar");
}
<div class="content-wrapper">
    <div class="container my-container" id="developers">
        @{
            Html.RenderPartial("Partial/DevelopersPartial", Model);
        }
    </div>
</div>
